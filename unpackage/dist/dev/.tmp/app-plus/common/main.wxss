














	/*每个页面公共css */
@font-face {
    font-family: 'vantfont';
    src: local('vantfont'),
        url(data:application/font-woff2;charset=utf-8;base64,) format('woff2'),
        url(data:application/font-woff;charset=utf-8;base64,) format('woff'),
        url(data:font/ttf;base64,) format('truetype');
    font-weight: normal;
    font-style: normal;
}
.vantfont {
  font-family: "vantfont" !important;
  font-size: 16px;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}
.vantfont:before {
	vertical-align: middle;
}
.vant-next:before {
  content: "\E661";
}
.vant-topic:before {
  content: "\E66B";
}

